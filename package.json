{
  "name": "raconteur-express",
  "version": "0.0.1",
  "description": "Raconteur middleware for express.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run create && npm run convert",
    "clean": "npm run clean-build && npm run clean-public",
    "clean-build": "rm -rf ./lib",
    "clean-public": "rm -rf ./public",
    "create": "npm run create-lib",
    "create-lib": "(mkdir ./lib || true)",
    "convert": "./node_modules/.bin/coffee --compile --output ./lib/ src/coffee/",
    "test": "npm run build && ./node_modules/.bin/mocha --reporter spec",
    "prepublish": "npm run build"
  },
  "keywords": [
    "raconteur",
    "blog",
    "markup",
    "markdown",
    "express",
    "middleware"
  ],
  "author": "Brekk Bockrath",
  "license": "ISC",
  "dependencies": {
    "debug": "^2.1.3",
    "express": "^4.12.3",
    "lodash": "^3.7.0",
    "parkplace": "^0.1.0",
    "promised-io": "^0.3.5"
  }
}
